# Kita Fees Backend - Environment Configuration
# Copy this file to .env and fill in your values

# Server
PORT=8081
CORS_ORIGINS=http://localhost:5175,http://localhost:5173

# Database
DATABASE_URL=postgres://kita:kita_dev_password@localhost:5432/kita?sslmode=disable&search_path=fees

# JWT
JWT_SECRET=your-secret-key-change-in-production

# SMTP (optional - for email notifications)
SMTP_HOST=
SMTP_PORT=587
SMTP_FROM=
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_USE_TLS=true
APP_BASE_URL=http://localhost:5175

# Banking Sync (FinTS)
# Generate with: openssl rand -base64 32
BANKING_ENCRYPTION_KEY=

# Cron API Token (for automated sync)
# Generate with: openssl rand -hex 32
CRON_API_TOKEN=
